# Phiên bản ứng dụng
REACT_APP_VERSION=v1.0.0

# Không tạo sourcemap để tối ưu build
GENERATE_SOURCEMAP=false

# Public URL (để trống nếu chạy localhost)
PUBLIC_URL=

# Tên base URL (nếu cần, thường để trống khi chạy localhost)
REACT_APP_BASE_NAME=

# Backend API URL (chạy trên localhost)
REACT_APP_API_URL=http://localhost:4000/

# Google Maps Key (nếu có)
REACT_APP_GOOGLE_MAPS_API_KEY=

# MapBox Token (nếu có)
REACT_APP_MAPBOX_ACCESS_TOKEN=

# Firebase Config (nếu dùng)
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=
REACT_APP_FIREBASE_MEASUREMENT_ID=

# AWS Config (nếu có)
REACT_APP_AWS_POOL_ID=
REACT_APP_AWS_APP_CLIENT_ID=

# Auth0 Config (nếu có)
REACT_APP_AUTH0_CLIENT_ID=
REACT_APP_AUTH0_DOMAIN=
